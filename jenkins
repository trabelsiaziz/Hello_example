
pipeline {
    agent any
    tools {
        jdk 'JDK21'
    }
    stages {
        stage('display message'){
            steps{
                echo "hello from github"
            }
    }
        stage('Checkout code') {
            steps {
                git branch: 'main', url: 'https://github.com/trabelsiaziz/Hello_example.git'
            }
        }
        stage('Compile code') {
            steps {
                sh 'javac hello.java'
            }
        }
        stage('Execute code') {
            steps {
                sh 'java hello'
            }
        }
    }
}
